{
  "expo": {
    "name": "TaxBridge",
    "slug": "taxbridge",
    "version": "5.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "taxbridge",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B5FFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ng.taxbridge.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "TaxBridge needs camera access to scan receipts and invoices for OCR processing.",
        "NSPhotoLibraryUsageDescription": "TaxBridge needs photo library access to import receipt images for tax documentation."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0B5FFF"
      },
      "package": "ng.taxbridge.app",
      "versionCode": 50000,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "TaxBridge needs camera access to scan receipts and invoices."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "fb2a2641-40b4-4741-bf52-b544d90ef1ba"
      }
    },
    "owner": "scardubu",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fb2a2641-40b4-4741-bf52-b544d90ef1ba"
    }
  }
}
